{
    "config": {
        "echo-server-addr" : {
            "help" : "IP address of echo server",
            "value" : "\"echo.mbedcloudtesting.com\""
        },
        "echo-server-port" : {
            "help" : "Port of echo server",
            "value" : "7"
        },
        "echo-server-discard-port" : {
            "help" : "Discard port of echo server",
            "value" : "9"
        },
        "echo-server-port-tls" : {
            "help" : "Port of echo server for TLS",
            "value" : "2007"
        },
        "echo-server-discard-port-tls" : {
            "help" : "Discard port of echo server for TLS",
            "value" : "2009"
        },
        "baud-rate": {
            "help": "baud rate",
            "value": 115200
        },
        "trace-level": {
            "help": "Note that excessive trace prints may mess up with Greentea parsing",
            "macro_name": "MBED_TRACE_MAX_LEVEL",
            "value": "TRACE_LEVEL_ERROR"
        }
    },
    "target_overrides": {
        "*": {
            "target.network-default-interface-type": "CELLULAR",
            "nsapi.default-cellular-sim-pin": "\"1234\"",
            "nsapi.default-cellular-apn": "\"internet\"",
            "nsapi.dns-response-wait-time": 30000,
            "ppp-cell-iface.apn-lookup": false,
            "cellular.use-apn-lookup": false,
            "mbed-trace.enable": true,
            "lwip.ipv4-enabled": true,
            "lwip.ipv6-enabled": true,
            "lwip.tcp-enabled": true,
            "lwip.ppp-enabled": false,
            "lwip.ethernet-enabled": false,
            "cellular.debug-at": false,
            "platform.stdio-baud-rate": 115200,
            "platform.default-serial-baud-rate": 115200,
            "cellular.radio-access-technology": 3,
            "drivers.uart-serial-rxbuf-size":1500,
            "drivers.uart-serial-txbuf-size":1500
        },
        "NRF52840_DK": {
            "QUECTEL_BG96.provide-default": true,
            "QUECTEL_BG96.tx": "p42",
            "QUECTEL_BG96.rx": "p40",
            "QUECTEL_BG96.cts": "p35",
            "QUECTEL_BG96.rts": "p36",
            "nsapi.default-cellular-apn": "\"internet\"",
            "cellular.radio-access-technology": 9,
            "cellular.use-apn-lookup": false,
            "ppp-cell-iface.apn-lookup": false,
            "lwip.ipv4-enabled": true,
            "lwip.ipv6-enabled": false,
            "lwip.ppp-enabled": false,
            "lwip.tcp-enabled": false,
            "cellular.debug-at": false
        },
        "NRF52840_DK": {
            "QUECTEL_BG96.provide-default": true,
            "QUECTEL_BG96.tx": "p42",
            "QUECTEL_BG96.rx": "p40",
            "QUECTEL_BG96.cts": "p35",
            "QUECTEL_BG96.rts": "p36",
            "nsapi.default-cellular-apn": "\"internet\"",
            "cellular.radio-access-technology": 9,
            "cellular.use-apn-lookup": false,
            "ppp-cell-iface.apn-lookup": false,
            "lwip.ipv4-enabled": true,
            "lwip.ipv6-enabled": false,
            "lwip.ppp-enabled": false,
            "lwip.tcp-enabled": false,
            "cellular.debug-at": false
        }
    }
}
